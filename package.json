{
  "name": "blog-api",
  "version": "1.0.0",
  "description": "Typescript, Express, and MongoDB API for a blog",
  "main": "src/server.ts",
  "scripts": {
    "start": "cross-env NODE_ENV=production node dist/server.js",
    "build": "tsc -p .",
    "dev": "cross-env NODE_ENV=development  ts-node-dev --respawn ./src/server.ts",
    "lint": "eslint --ignore-path .gitignore --ext .ts src/",
    "lint:fix": "npm run lint -- --fix"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/oburakergen/blog-api.git"
  },
  "keywords": [
    "typescript",
    "express",
    "mongoose",
    "aws-s3",
    "api",
    "morgan",
    "winston"
  ],
  "author": "osman burak ergen",
  "license": "ISC",
  "devDependencies": {
    "@types/consolidate": "^0.14.4",
    "@types/express": "^4.17.21",
    "@types/mongoose": "^5.11.97",
    "@types/node": "^20.11.28",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/yamljs": "^0.2.34",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "lint-staged": "^15.2.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.556.0",
    "@types/morgan": "^1.9.9",
    "consolidate": "^1.0.3",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "mongoose": "^8.2.0",
    "morgan": "^1.10.0",
    "slugify": "^1.6.6",
    "swagger-ui-express": "^5.0.0",
    "winston": "^3.11.0",
    "yamljs": "^0.3.0",
    "zod": "^3.22.5"
  }
}
